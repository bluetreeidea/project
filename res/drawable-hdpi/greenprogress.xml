<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
<item android:id="@android:id/background">
 
    
    
    <shape>
        <corners android:radius="0dip" 
         />
        <gradient
                android:startColor="#ff9d9e9d"
                android:centerColor="#ff5a5d5a"
                android:centerY="4.0"
                android:endColor="#ff747674"
                android:angle="270"
        />
    </shape>
</item>

<item android:id="@android:id/secondaryProgress">
    <clip>
        <shape>
            <corners android:radius="0dip" />
            <gradient
                    android:startColor="#F2595600"
                    android:centerColor="#F2595800"
                    android:centerY="4.0"
                    android:endColor="#F7201D00"
                    android:angle="270"
            />
        </shape>
    </clip>
</item>
<item
    android:id="@android:id/progress"
>
    <clip>
        <shape>
            <corners
                android:radius="0dip" />
            <gradient
                android:startColor="#F25950"
                android:endColor="#F25956"
                android:angle="270" />
        </shape>
    </clip>
</item>

</layer-list>